{
  "manifest_version": 3,
  "name": "Agent AI Browser Bridge",
  "version": "1.0",
  "description": "Bridge between browser and Agent AI",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "http://localhost/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
